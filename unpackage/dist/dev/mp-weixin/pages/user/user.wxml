<scroll-view class="page"><block wx:if="{{hasLogin}}"><view class="content"><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">个人中心</view></cu-custom><view class="cu-list menu sm-border card-menu margin-top"><view class="cu-item cu-form-group"><view class="content"><text class="icon-circlefill text-grey"></text><text class="text-grey">普通选择</text></view><picker value="{{index}}" range="{{picker}}" data-event-opts="{{[['change',[['PickerChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(index>-1?picker[index]:'单选项')+''}}</view></picker></view><view class="cu-item arrow"><view class="content"><image class="png" src="/static/logo.png" mode="aspectFit"></image><text class="text-grey">日期选项</text></view><picker mode="date" value="{{date}}" start="2015-09-01" end="2020-09-01" data-event-opts="{{[['change',[['DateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+date+''}}</view></picker></view><view class="cu-item"><view class="content padding-tb-sm"><view><text class="icon-clothesfill text-blue margin-right-xs"></text>多行Item</view><view class="text-gray text-sm"><text class="icon-infofill margin-right-xs"></text>小目标还没有实现！</view></view><view class="action"><switch class="{{['switch-sex',skin?'checked':'']}}" checked="{{skin?true:false}}" data-event-opts="{{[['change',[['SwitchSex',['$event']]]]]}}" bindchange="__e"></switch></view></view></view><view class="cu-list sm-border card-menu "><button data-event-opts="{{[['tap',[['bindLogout',['$event']]]]]}}" class="cu-btn block bg-gradual-blue margin-tb-sm lg" bindtap="__e">退出登录</button></view></view></block></scroll-view>