<view class="content" bind:__l="__l"><view class="input-group"><cu-custom bgColor="bg-gradual-blue shadow" isBack="{{true}}" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">注册中心</view></cu-custom><view class="cu-form-group margin-top shadow"><text class="title">账号 ：</text><input focus clearable placeholder="请输入账号" data-event-opts="{{[['input',[['__set_model',['account','$event',[]]]]]]}}" value="{{account}}" bindinput="__e"/></view><view class="cu-form-group shadow"><view class="title">密码 ：</view><m-input bind:input="__e" type="password" displayable placeholder="请输入密码" value="{{password}}" data-event-opts="{{[['^input',[['__set_model',['password','$event']]]]]}}"></m-input></view><view class="cu-form-group shadow"><text class="title">邮箱 ：</text><input clearable placeholder="请输入邮箱" data-event-opts="{{[['input',[['__set_model',['email','$event',[]]]]]]}}" value="{{email}}" bindinput="__e"/></view></view><view class="btn-row"><button class="cu-btn block bg-gradual-blue margin-tb-sm lg shadow" type="primary" data-event-opts="{{[['tap',[['register',['$event']]]]]}}" bindtap="__e">注 册</button></view></view>